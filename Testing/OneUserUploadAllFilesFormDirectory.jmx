            <stringProp name="BeanShellSampler.query">int fileNumber = 1;

void findFiles(String directoryName) {
    File directory = new File(directoryName);

    // get all the files from a directory
    File[] fList = directory.listFiles();
    for (File file : fList) {
        if (file.isFile()) {
            vars.put(&quot;sourcePath_&quot; + fileNumber.toString(), file.getAbsolutePath());
            fileNumber++;
        } else if (file.isDirectory()) {
            findFiles(file.getAbsolutePath());
        }
    }
}

var filesDirectory = vars.get(&quot;FilesDirectory&quot;);
findFiles(filesDirectory);</stringProp>
